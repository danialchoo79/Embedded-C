#include <stdint.h>
#include <stdio.h>

struct DataSet
{
	char data1;
	int data2;
	char data3;
	short data4;
};

int main(void)
{
	struct DataSet data;
	data.data1 = 0x11;
	data.data2 = 0xFFFFEEEE;
	data.data3 = 0x22;
	data.data4 = 0xABCD;

	printf("Memory Address		Content \n");
	printf("=============================\n");

	return 0;
}
